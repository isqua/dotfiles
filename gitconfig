[push]
	default = current
[core]
	editor = vim
	pager = less
[color]
	ui = true
[alias]
	co = checkout
	empty = commit --allow-empty -m
	temp = commit --no-verify -v -m
	fix = commit --fixup
	squash = rebase --autosquash -i
	diffs = diff --staged
	orphan = checkout --orphan
